  <AlienRace.BackstoryDef>
    <defName>RH_TET_Skaven_MasterMoulder</defName>
    <title>master moulder</title>
    <titleShort>master moulder</titleShort>
    <baseDescription>[PAWN_nameDef] is among the most skilled among those in Clan Moulder. [PAWN_pronoun] is a true master at [PAWN_possessive] vile and gruesome work.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Male</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Intellectual</defName>
        <amount>4</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>6</amount>
      </li>
    </skillGains>
    <forcedTraits>
