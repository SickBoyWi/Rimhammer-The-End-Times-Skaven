        <defName>Psychopath</defName>
        <chance>90</chance>
      </li>
      <li>
        <defName>Industriousness</defName>
        <degree>2</degree>
        <chance>50</chance>
      </li>
    </forcedTraits>
    <forcedHediffs>
      <li>RH_TET_SkavenMoulderWarpSpine</li>
    </forcedHediffs>
    <spawnCategories>
      <li>RH_TET_SkavenMoulder</li>
    </spawnCategories>
    <requiredWorkTags>
      <li>Intellectual</li>
      <li>Violent</li>
    </requiredWorkTags>
