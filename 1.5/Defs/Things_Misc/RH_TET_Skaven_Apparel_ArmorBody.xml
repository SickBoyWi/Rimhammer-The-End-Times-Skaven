		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(0.33,0.33,0.33)</only>
				</li>
				<li>
					<weight>10</weight>
					<only>(0.65,0.65,0.65)</only>
				</li>
				<li>
					<weight>6</weight>
					<min>(0.3,0.3,0.3)</min>
					<max>(0.5,0.5,0.5)</max>
				</li>
			</options>
		</colorGenerator>
		<tradeability>None</tradeability>
